{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "ignorePatterns": ["dist"],
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": 1,
        "no-unused-vars": 1
    }
}
